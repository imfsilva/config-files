{
  "workbench.colorTheme": "Codely Dark",
  "workbench.editor.enablePreview": false,
  "editor.fontSize": 16,
  "editor.fontFamily": "'IBM Plex Mono', Consolas, Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.fastScrollSensitivity": 4,
  "editor.renderWhitespace": "none",
  "explorer.compactFolders": false,
  "explorer.autoReveal": false,
  "window.zoomLevel": 0,
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[vue]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "gitlens.views.repositories.files.layout": "list",
  "gitlens.currentLine.format": "${author}, ${agoOrDate}",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontFamily": "'IBM Plex Mono'"
}
